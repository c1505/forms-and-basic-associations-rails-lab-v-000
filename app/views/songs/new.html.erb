<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist %>
  <%= f.text_field :artist_name, list: "artist_autocomplete" %>
  <datalist id="artist_autocomplete">
    <% Artist.all.each do |artist| %>
      <option value="<%= artist.name %>">
    <% end %>
  </datalist>



  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  
  <p><input id= "song_notes_1" name="song_notes[]"></p>
  <p><input id= "song_notes_2" name="song_notes[]"></p>
  <p><input id= "song_notes_3" name="song_notes[]"></p>

  
  <%= f.submit %>
<% end %>

